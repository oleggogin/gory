                resolve();
            }).then(_get(FotooboiSettings.prototype.__proto__ || Object.getPrototypeOf(FotooboiSettings.prototype), 'init', this).call(this));
        }
    }, {
        key: 'launch',
        value: function launch() {
            if (this.$height.val()) {
                this.$height.trigger('keyup');
                this.$height.trigger('change');
                this.disappearOnFocus('justLaunched', this.$height);
            }
            if (this.$width.val()) {
                this.$width.trigger('keyup');
                this.$width.trigger('change');
                this.disappearOnFocus('justLaunched', this.$width);
            }
            if (this.$material.val()) {
                this.$material.trigger('change');
            }
            if (this.$color_proof.val()) {
                this.$color_proof.trigger('change');
            }
            if (this.$wallpaper_companions.val()) {
                this.$wallpaper_companions.trigger('change');
            }
            if (this.$companion_width_cm.val()) {
                this.$companion_width_cm.trigger('keyup');
                this.$companion_width_cm.trigger('change');
                this.disappearOnFocus('justLaunched', this.$companion_width_cm);
            }
            if (this.$companion_height_cm.val()) {
                this.$companion_height_cm.trigger('keyup');
                this.$companion_height_cm.trigger('change');
                this.disappearOnFocus('justLaunched', this.$companion_height_cm);
            }
            if (this.$installation.val()) {
                this.$installation.trigger('change');
            }
        }
    }, {
        key: 'proportionCalc',
        value: function proportionCalc(old_x, new_x, old_y) {
            return old_y * new_x / old_x;
        }
    }, {
        key: 'putRectangleInRectangle',
        value: function putRectangleInRectangle(w1, h1, w2, h2) {
            var w3 = void 0;
            var h3 = void 0;

            w3 = w1;
            h3 = this.proportionCalc(w2, w3, h2);

            if (h3 <= h1) {
            } else {
                h3 = h1;
                w3 = this.proportionCalc(h2, h3, w2);
            }

            var x = (w1 - w3) / 2;
            var y = (h1 - h3) / 2;

            var cropDefinition = {
                x: x,
                y: y,
                width: w3,
                height: h3
            };

            return cropDefinition;
        }
    }, {
        key: 'updateFormByPreview',
        value: function updateFormByPreview() {
            var cropDefinition = this.preview.image1.$img.cropper('getData');

            var oldWidth = this.$width.val();
            var oldHeight = this.$height.val();

            this.$x.val(Math.round(cropDefinition.x / this.pixelsInCm));
            this.$y.val(Math.round(cropDefinition.y / this.pixelsInCm));
            this.$width.val(Math.round(cropDefinition.width / this.pixelsInCm));
            this.$height.val(Math.round(cropDefinition.height / this.pixelsInCm));

            if (oldWidth != this.$width.val()) {
                this.$width.animate({'opacity': 0.5}, 200).animate({'opacity': 1}, 200);

                this.$width.trigger('change');
            }

            if (oldHeight != this.$height.val()) {
                this.$height.animate({'opacity': 0.5}, 200).animate({'opacity': 1}, 200);

                this.$height.trigger('change');
            }

            this.$x.trigger('change');
            this.$y.trigger('change');
        }
    }, {
        key: 'updatePreviewBySize',
        value: function updatePreviewBySize() {
            console.log('update preview');
            var width = this.$width.val();
            var height = this.$height.val();
            if (!(width && width == parseInt(width) && height && height == parseInt(height))) {
                return;
            }

            var imageElement = this.preview.$img[0];

            var cropDefinition = this.putRectangleInRectangle(imageElement.naturalWidth, imageElement.naturalHeight, width, height);

            if (cropDefinition.width == imageElement.naturalWidth) {
                this.$img_width.val(width);
                this.$img_height.val(Math.round(this.proportionCalc(imageElement.naturalWidth, width, imageElement.naturalHeight)));
                this.pixelsInCm = imageElement.naturalWidth / width;
            } else {
                this.$img_height.val(height);
                this.$img_width.val(Math.round(this.proportionCalc(imageElement.naturalHeight, height, imageElement.naturalWidth)));
                this.pixelsInCm = imageElement.naturalHeight / height;
            }

            this.$x.val(Math.round(cropDefinition.x / this.pixelsInCm));
            this.$y.val(Math.round(cropDefinition.y / this.pixelsInCm));

            this.$img_width.trigger('change');
            this.$img_height.trigger('change');
            this.$x.trigger('change');
            this.$y.trigger('change');

            this.preview.setCrop(cropDefinition);
        }
    }, {
        key: '$width',
        get: function get() {
            return this.get('$width');
        }
    }, {
        key: '$height',
        get: function get() {
            return this.get('$height');
        }
    }, {
        key: '$img_width',
        get: function get() {
            return this.get('$img_width');
        }
    }, {
        key: '$img_height',
        get: function get() {
            return this.get('$img_height');
        }
    }, {
        key: '$x',
        get: function get() {
            return this.get('$x');
        }
    }, {
        key: '$y',
        get: function get() {
            return this.get('$y');
        }
    }, {
        key: '$material',
        get: function get() {
            return this.get('$material');
        }
    }, {
        key: '$color_proof',
        get: function get() {
            return this.get('$color_proof');
        }
    }, {
        key: '$wallpaper_companions',
        get: function get() {
            return this.get('$wallpaper_companions');
        }
    }, {
        key: '$companion_width_cm',
        get: function get() {
            return this.get('$companion_width_cm');
        }
    }, {
        key: '$companion_height_cm',
        get: function get() {
            return this.get('$companion_height_cm');
        }
    }, {
        key: '$installation',
        get: function get() {
            return this.get('$installation');
        }
    }]);

    return FotooboiSettings;
}(DetailProductSettings);

// маленький жс, который будет просто тогглить видимость доп полей

'use strict';

var _createClass = function () { function defineProperties(target, props) { for (var i = 0; i < props.length; i++) { var descriptor = props[i]; descriptor.enumerable = descriptor.enumerable || false; descriptor.configurable = true; if ("value" in descriptor) descriptor.writable = true; Object.defineProperty(target, descriptor.key, descriptor); } } return function (Constructor, protoProps, staticProps) { if (protoProps) defineProperties(Constructor.prototype, protoProps); if (staticProps) defineProperties(Constructor, staticProps); return Constructor; }; }();

function _classCallCheck(instance, Constructor) { if (!(instance instanceof Constructor)) { throw new TypeError("Cannot call a class as a function"); } }

var Buy = function () {
    function Buy(config) {
        _classCallCheck(this, Buy);

        this.priceSelector = null;
        this.oldPriceSelector = null;
        this.priceValidationSelector = null;
        this.buyButtonSelector = null;
        this.checkoutButtonSelector = null;
        this.cartSelector = null;
        this.formSelector = null;

        for (var key in config) {
            this[key] = config[key];
        }

        this.getCache = {};
        this.lastUpdatePriceParams = {};

        this.watching = [];

        this.init();
    }

    _createClass(Buy, [{
        key: 'getElementBySelector',
        value: function getElementBySelector(selector) {
            var value = void 0;

            if (typeof selector === 'function') {
                value = selector();
            } else {
                value = $(selector);
            }

            return value;
        }
    }, {
        key: 'get',
        value: function get(name) {
            if (!this.getCache[name]) {
                var value = void 0;

                switch (name) {
                    case '$price':
                        value = this.getElementBySelector(this.priceSelector);
                        break;
                    case '$oldPrice':
                        value = this.getElementBySelector(this.oldPriceSelector);
                        break;
                    case '$priceValidation':
                        value = this.getElementBySelector(this.priceValidationSelector);
                        break;
                    case '$buy':
                        value = this.getElementBySelector(this.buyButtonSelector);
                        break;
                    case '$form':
                        value = this.getElementBySelector(this.formSelector);
                        break;
                    case '$checkout':
                        value = this.getElementBySelector(this.checkoutButtonSelector);
                        break;
                    case '$cart':
                        value = this.getElementBySelector(this.cartSelector);
                        break;
                    case 'deferredAction':
                        value = new DeferredAction();
                        break;
                    default:
                        throw new Error('No getter for ' + name);
                }

                this.getCache[name] = value;
            }

            return this.getCache[name];
        }
    }, {
        key: 'init',
        value: function init() {
            var self = this;

            var $form = this.$form;

            $form.on('afterInit', function () {
                self.$buy.on('click', function (e) {
                    e.preventDefault();
                    self.addToCart();
                });

                $form.on('beforeSubmit', function () {
                    return self.beforeSubmit();
                });
            });
        }
    }, {
        key: 'addToCart',
        value: function addToCart() {
            this.submitAction = 'add-to-cart';
            this.$form.submit();
        }
    }, {
        key: 'updateCart',
        value: function updateCart() {
            var self = this;

            $.ajax({
                type: 'get',
                url: '/-/cart/cart/get-contents'
            }).done(function (data) {
                if (data.html) {
                    self.$cart.replaceWith(data.html);
                }
            });
        }
    }, {
        key: 'beforeSubmit',
        value: function beforeSubmit() {
            var self = this;

            if (this.submitAction === 'update-price') {
                $.ajax({
                    type: 'get',
                    url: '/-/cart/cart/get-price',
                    data: this.$form.serializeArray()
                }).done(function (data) {
                    if (data.success) {
                        self.$price.text(data.price);
                        if (data.oldPrice) {
                            self.$oldPrice.text(data.oldPrice);
                        }
                        self.$priceValidation.toggleClass('-active', false);
                    } else {
                        if (data.errors) {
                            var errors = Object.values(data.errors);
                            var error = errors[0][0];

                            if (error) {
                                self.$priceValidation.text(error);
                                self.$priceValidation.toggleClass('-active', true);
                            }
                        }

                        self.$price.text('...');
                        self.$oldPrice.text('');
                    }

                    self.$form.trigger('buy:priceUpdated');
                }).fail(function () {
                    self.$price.text('...');
                }).always(function () {
                    self.$price.toggleClass('-updating', false);
                });
            } else if (this.submitAction === 'add-to-cart') {
                if (self.$buy.attr('disabled')) {
                    return false;
                }
                this.$buy.prop('disabled', true);
                $.ajax({
                    type: 'post',
                    url: '/-/cart/cart/add',
                    data: this.$form.serializeArray()
                }).done(function (data) {
                    if (data.success) {
                        self.$buy.addClass('-hidden');
                        self.$checkout.removeClass('-hidden');
                        self.updateCart();
                    } else {
                        console.log('buy failure');
                    }
                }).fail(function () {
                    console.log('request failure');
                }).always(function () {
                    self.$buy.prop('disabled', false);
                });
            }

            return false;
        }
    }, {
        key: 'watchChanges',
        value: function watchChanges($element) {
            var self = this;

            this.watching.push($element);

            $element.on('change', function () {
                self.updatePrice();
            });
        }
    }, {
        key: 'isArraysEquivalent',
        value: function isArraysEquivalent(a1, a2) {
            return a1.length === a2.length && a1.every(function (v, i) {
                return v === a2[i];
            });
        }
    }, {
        key: 'isEquivalent',
        value: function isEquivalent(a, b) {
            var aProps = Object.getOwnPropertyNames(a);
            var bProps = Object.getOwnPropertyNames(b);

            if (aProps.length !== bProps.length) {
                return false;
            }

            for (var i = 0; i < aProps.length; i++) {
                var propName = aProps[i];

                if (a[propName] !== b[propName]) {
                    return false;
                }
            }

            return true;
        }
    }, {
        key: 'updatePrice',
        value: function updatePrice() {
            var self = this;

            self.$price.toggleClass('-updating', true);

            this.deferredAction.fire('update-price', 1000, function () {
                var params = self.updatePriceParams;

                if (self.isArraysEquivalent(params, self.lastUpdatePriceParams)) {
                    self.$price.toggleClass('-updating', false);
                    return;
                }

                self.lastUpdatePriceParams = params;

                self.submitAction = 'update-price';
                self.$form.submit();
            });
        }
    }, {
        key: 'deferredAction',
        get: function get() {
            return this.get('deferredAction');
        }
    }, {
        key: '$cart',
        get: function get() {
            return this.get('$cart');
        }
    }, {
        key: '$form',
        get: function get() {
            return this.get('$form');
        }
    }, {
        key: '$checkout',
        get: function get() {
            return this.get('$checkout');
        }
    }, {
        key: '$buy',
        get: function get() {
            return this.get('$buy');
        }
    }, {
        key: '$price',
        get: function get() {
            return this.get('$price');
        }
    }, {
        key: '$oldPrice',
        get: function get() {
            return this.get('$oldPrice');
        }
    }, {
        key: '$priceValidation',
        get: function get() {
            return this.get('$priceValidation');
        }
    }, {
        key: 'updatePriceParams',
        get: function get() {
            var data = [];
            var _iteratorNormalCompletion = true;
            var _didIteratorError = false;
            var _iteratorError = undefined;

            try {
                for (var _iterator = this.watching[Symbol.iterator](), _step; !(_iteratorNormalCompletion = (_step = _iterator.next()).done); _iteratorNormalCompletion = true) {
                    var $element = _step.value;

                    data.push($element.val());
                }
            } catch (err) {
                _didIteratorError = true;
                _iteratorError = err;
            } finally {
                try {
                    if (!_iteratorNormalCompletion && _iterator.return) {
                        _iterator.return();
                    }
                } finally {
                    if (_didIteratorError) {
                        throw _iteratorError;
                    }
                }
            }

            return data;
        }
    }]);

    return Buy;
}();
//# sourceMappingURL=buy.js.map

    }, {
        key: 'init',
        value: function init() {
            var self = this;
            return new Promise(function (resolve, reject) {
                self.disappearOnFocus('attachHandlers', self.$width);
                self.disappearOnFocus('attachHandlers', self.$height);

                self.$width.on('keyup', function () {
                    self.disappearOnFocus('changed', self.$width);
                    self.updatePreviewBySize();
                });
                self.$height.on('keyup', function () {
                    self.updatePreviewBySize();
                    self.disappearOnFocus('changed', self.$height);
                });


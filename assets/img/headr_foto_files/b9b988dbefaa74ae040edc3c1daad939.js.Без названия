$(function () {
    $('[data-als2-image-fit-container]').each(function(i, element) {
        var $widget = $(element)
        $widget.addClass('als2-image-fit-container')

        var config = $widget.data('als2-image-fit-container')
        config = config ? config : {}
        config.breakpoints = config.breakpoints ? config.breakpoints : {}

        // normalize keys to int
        var breakpointsObject = {}
        for (let width in config.breakpoints) {
            breakpointsObject[parseInt(width)] = config.breakpoints[width]
        }
        config.breakpoints = breakpointsObject

        config.$referenceContainer = $widget.parent()
        config.$image = $widget.find('img')
        config.$widget = $widget

        updateImageMaxHeight(config)

        var lastWindowWidth = window.innerWidth
        $(window).resize(function() {
            var windowWidth = window.innerWidth
            for (var width in config.breakpoints) {
                // console.log(lastWindowWidth, width, windowWidth)
                if (
                    (lastWindowWidth >= width && windowWidth < width)
                    || (lastWindowWidth < width && windowWidth >= width)
                ) {
                    updateImageMaxHeight(config)
                    break
                }
            }
            lastWindowWidth = windowWidth
        })

    })

    return
    
    function updateImageMaxHeight(config) {
        let $referenceContainer = config.$referenceContainer
        let $widget = config.$widget
        let $image = config.$image

        if (isFixedForCurrentWidth(config)) {
            var height = $referenceContainer.height() + 'px'
            $image.css({
                'max-height': height,
            })
            $widget.addClass('-fixed')
        } else {
            $image.css({
                'max-height': '',
            })
            $widget.removeClass('-fixed')
        }

    }

    function isFixedForCurrentWidth(config) {
        // по умолчанию высота картинки определяет высоту контейнера, высота контейнера не фиксирована
        var fixed = false
        var windowWidth = window.innerWidth
        for (var width in config.breakpoints) {
            if (width >= windowWidth) break

            var settings = config.breakpoints[width]

            if (!settings) continue

            if ($.inArray('fixed-height', settings) > -1) {
                fixed = true
            } else if ($.inArray('fluid-height', settings) > -1) {
                fixed = false
            } else {
                // no changes from previous value
            }
        }

        return fixed
    }
})
;;
"use strict";

var _createClass = function () {
    function defineProperties(target, props) {
        for (var i = 0; i < props.length; i++) {
            var descriptor = props[i];
            descriptor.enumerable = descriptor.enumerable || false;
            descriptor.configurable = true;
            if ("value" in descriptor) descriptor.writable = true;
            Object.defineProperty(target, descriptor.key, descriptor);
        }
    }

    return function (Constructor, protoProps, staticProps) {
        if (protoProps) defineProperties(Constructor.prototype, protoProps);
        if (staticProps) defineProperties(Constructor, staticProps);
        return Constructor;
    };
}();

var _get = function get(object, property, receiver) {
    if (object === null) object = Function.prototype;
    var desc = Object.getOwnPropertyDescriptor(object, property);
    if (desc === undefined) {
        var parent = Object.getPrototypeOf(object);
        if (parent === null) {
            return undefined;
        } else {
            return get(parent, property, receiver);
        }
    } else if ("value" in desc) {
        return desc.value;
    } else {
        var getter = desc.get;
        if (getter === undefined) {
            return undefined;
        }
        return getter.call(receiver);
    }
};

function _classCallCheck(instance, Constructor) {
    if (!(instance instanceof Constructor)) {
        throw new TypeError("Cannot call a class as a function");
    }
}

function _possibleConstructorReturn(self, call) {
    if (!self) {
        throw new ReferenceError("this hasn't been initialised - super() hasn't been called");
    }
    return call && (typeof call === "object" || typeof call === "function") ? call : self;
}

function _inherits(subClass, superClass) {
    if (typeof superClass !== "function" && superClass !== null) {
        throw new TypeError("Super expression must either be null or a function, not " + typeof superClass);
    }
    subClass.prototype = Object.create(superClass && superClass.prototype, {
        constructor: {
            value: subClass,
            enumerable: false,
            writable: true,
            configurable: true
        }
    });
    if (superClass) Object.setPrototypeOf ? Object.setPrototypeOf(subClass, superClass) : subClass.__proto__ = superClass;
}


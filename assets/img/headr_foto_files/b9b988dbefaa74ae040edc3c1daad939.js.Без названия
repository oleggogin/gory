var DetailProductStickerSettings = function (_DetailProductSetting) {
    _inherits(DetailProductStickerSettings, _DetailProductSetting);

    function DetailProductStickerSettings() {
        _classCallCheck(this, DetailProductStickerSettings);

        var _this = _possibleConstructorReturn(this, (DetailProductStickerSettings.__proto__ || Object.getPrototypeOf(DetailProductStickerSettings)).call(this));


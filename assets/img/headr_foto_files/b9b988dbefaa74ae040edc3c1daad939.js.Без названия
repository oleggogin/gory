$(function () {
    'use strict'

    var widget = {
        additionalDataGetter: null,
        $cityInput: null,
        $deliveryContainer: null,
        attach: function ($cityInput, $deliveryContainer, updateOnAttach) {
            var self = this

            self.$cityInput = $cityInput
            self.$deliveryContainer = $deliveryContainer

            self.$cityInput.on('change', function () {
                self.loadDeliveryInfo(self.$cityInput.val(), self.$deliveryContainer)
            })

            if (typeof updateOnAttach === "undefined") {
                updateOnAttach = true
            }

            if (updateOnAttach) {
                self.update()
            }
        },

        update: function() {
            var self = this

            self.loadDeliveryInfo(self.$cityInput.val(), self.$deliveryContainer)
        },

        loadDeliveryInfo: function(cityId, $deliveryContainer) {
            var self = this
            var data = {id: cityId}

            if (self.additionalDataGetter) {
                data.additional = self.additionalDataGetter()
            }

            $.ajax({
                type: 'get',
                url: '/-/contacts/get-delivery-info-by-city-id',
                data: data,
            })
                .done(function (data) {
                    self.updateDeliveryInfo(data, $deliveryContainer)
                })
                .fail(function () {
                    self.updateDeliveryInfo([], $deliveryContainer)
                })
        },

        updateDeliveryInfo: function (data, $deliveryContainer) {
            $deliveryContainer.find('[data-delivery-sid]').each(function () {
                var $infoItem = $(this)
                // infoType - это 'cost' или 'time', то есть поле из info, которое мы должны
                // выводить в this
                var infoType = $infoItem.data('info')

                var gotInfo

                for (var k in data) {
                    var delivery = data[k]

                    if (delivery.sid !== $infoItem.data('delivery-sid')) {
                        continue
                    }

                    if (typeof delivery.info[infoType] === "undefined") {
                        continue
                    }

                    if (delivery.info['error'] === true) {
                        continue
                    }

                    gotInfo = true

                    if (infoType === 'cost') {
                        $infoItem.html(delivery.info[infoType] + ' ' + delivery.info['currency'])
                    } else {
                        $infoItem.html(delivery.info[infoType])
                    }
                }

                if (!gotInfo) {
                    $infoItem.html('&mdash;')
                }
            })
        }

    }

    // быстрый костыль пока нет require
    // цель - один управляющий код, много инстансов
    var body = $('body')[0]
    if (!body['cronfy-ht-widgets']) {
        body['cronfy-ht-widgets'] = {}
    }

    // оставим для повторного использования: если будут динамически появляться новые виджеты этого типа,
    // можно будет сделать $('body')[0][]['cronfy-ht-widgets']['als2-map'].attach($(...))
    body['cronfy-ht-widgets']['als2-delivery-updater'] = widget
})
;;

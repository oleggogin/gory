        return _this;
    }

    _createClass(DetailProductStickerSettings, [{
        key: 'getters',
        value: function getters() {
            var self = this;

            var getters = _get(DetailProductStickerSettings.prototype.__proto__ || Object.getPrototypeOf(DetailProductStickerSettings.prototype), 'getters', this).call(this);

            getters.push({
                $color1: function $color1() {
                    return self.$form.find('select[name*="[color1]"]');
                },
                $width: function $width() {
                    return self.$form.find('input[name*="[width_cm]"]');
                },
                $height: function $height() {
                    return self.$form.find('input[name*="[height_cm]"]');
                },
                $sizeSelect: function $sizeSelect() {
                    return self.$form.find('[data-object=size-select]');
                },
                $slider: function $slider() {
                    return self.$form.find('[data-object=slider]');
                },
                $mirror: function $mirror() {
                    return self.$form.find('input[name*="[mirror]"]');
                },
                $ownSize: function $ownSize() {
                    return self.$form.find('[data-object=own-size]');
                }
            });

            return getters;
        }
    }, {
        key: 'launch',
        value: function launch() {
            this.$color1.trigger('change');
            this.$sizeSelect.trigger('change');

            _get(DetailProductStickerSettings.prototype.__proto__ || Object.getPrototypeOf(DetailProductStickerSettings.prototype), 'launch', this).call(this);
        }
    }, {
        key: 'superInit',
        value: function superInit() {
            return _get(DetailProductStickerSettings.prototype.__proto__ || Object.getPrototypeOf(DetailProductStickerSettings.prototype), 'init', this).call(this);
        }
    }, {
        key: 'init',
        value: function init() {
            var self = this;
            return new Promise(function (resolve, reject) {
                self.initMirror();
                self.initSize();
                self.initSizeSelect();
                self.initSlider();
                self.attachToPreview();
                resolve();
            }).then(function () {
                return self.superInit();
            });
        }
    }, {
        key: 'initMirror',
        value: function initMirror() {
            var self = this;

            this.$mirror.on('change', function () {
                var $checkbox = $(this);
                self.$mirror.prop('checked', $checkbox.is(':checked'));
            });
        }
    }, {
        key: 'tuneBackgroundColor',
        value: function tuneBackgroundColor() {
            var self = this;

            var $selectedOption = self.$color1.find("option:selected");

            if ($selectedOption.data('color') === 'white') {
                self.preview.$container.css({ 'background-color': 'grey' });
            } else {
                self.preview.$container.css({ 'background-color': 'white' });
            }
        }
    }, {
        key: 'attachToPreview',
        value: function attachToPreview() {
            var self = this;

            this.$mirror.on('change', function () {
                self.preview.toggleMirror(self.$mirror.val());
            });

            this.$color1.on('change', function () {
                var $selectedOption = self.$color1.find("option:selected");

                self.preview.image1.setFilter($selectedOption.data('filter'));

                self.tuneBackgroundColor();
            });
        }
    }, {
        key: 'updateSizeByAnother',
        value: function updateSizeByAnother($source) {
            if ($source === this.$height) {
                this.$width.val(Math.round(this.widthToHeight * this.$height.val()));
            } else if ($source == this.$width) {
                this.$height.val(Math.round(this.$width.val() / this.widthToHeight));
            } else {
                throw "Unknown size source";
            }
            this.$slider.slider('value', this.get('$width').val());
        }
    }, {
        key: 'initSize',
        value: function initSize() {
            var self = this;
            this.widthToHeight = this.$width.val() / this.$height.val();

            if (this.$form.data('size-proportions') !== false) {

                this.get('$width').on('change', function () {
                    self.updateSizeByAnother(self.$width);
                });
                this.get('$height').on('change', function () {
                    self.updateSizeByAnother(self.$height);
                });

                this.$height.on('keyup', function () {
                    var value = self.$height.val();
                    if (value && value == parseInt(value)) {
                        self.updateSizeByAnother(self.$height);
                    }
                });
                this.$width.on('keyup', function () {
                    var value = self.$width.val();
                    if (value && value == parseInt(value)) {
                        self.updateSizeByAnother(self.$width);
                    }
                });
            } else {
                this.$slider.closest('.stickerSlider').hide();
            }
        }
    }, {
        key: 'initSizeSelect',
        value: function initSizeSelect() {
            var self = this;
            this.$sizeSelect.on('change', function () {
                if (self.$sizeSelect.val() == 'own') {
                    self.$ownSize.show();
                    return;
                } else {
                    self.$ownSize.hide();
                }

                var $selectedOption = self.$sizeSelect.find("option:selected");

                self.$width.val($selectedOption.data('width'));
                self.$height.val($selectedOption.data('height'));

                self.widthToHeight = self.$width.val() / self.$height.val();

                self.$width.trigger('change');
                self.$height.trigger('change');
            });
        }
    }, {
        key: 'initSlider',
        value: function initSlider() {
            var min = this.$slider.data('min-width');
            var max = 600;
            var roundTo = 1;
            var forceChange = void 0;
            var startWidth = void 0;
            var startHeight = void 0;
            var self = this;

            this.$slider.slider({
                value: min,
                min: min,
                max: max,
                step: 10,
                start: function start() {
                    forceChange = false;

                    startWidth = self.get('$width').val();
                    startHeight = self.get('$height').val();
                    if (!startWidth) {
                        startWidth = min;
                        forceChange = true;
                    }
                    if (!startHeight) {
                        startHeight = min;
                        forceChange = true;
                    }
                },
                slide: function slide(e, ui) {
                    var newWidth = ui.value;
                    var newHeight = void 0;

                    if (newWidth == startWidth) {
                        newHeight = startHeight;
                    } else {
                        newHeight = Math.round(startHeight / startWidth * newWidth / roundTo) * roundTo;
                    }

                    self.get('$width').val(newWidth);
                    self.get('$height').val(newHeight);
                },
                stop: function stop(e, ui) {
                    if (forceChange || ui.value != startWidth) {
                        self.get('$width').trigger('change');
                        self.get('$height').trigger('change');
                    }
                }
            });
        }
    }, {
        key: 'renderColorOption',
        value: function renderColorOption(item) {
            if (!item.id) {
                return item.text;
            }

            var $element = $(item.element);

            return $('<div class="variant">' + '<span class="rectangle" style="background-color: ' + $element.data('color') + '"></span> ' + $element.text() + '</div>');
        }
    }, {
        key: '$ownSize',
        get: function get() {
            return this.get('$ownSize');
        }
    }, {
        key: '$color1',
        get: function get() {
            return this.get('$color1');
        }
    }, {
        key: '$slider',
        get: function get() {
            return this.get('$slider');
        }
    }, {
        key: '$width',
        get: function get() {
            return this.get('$width');
        }
    }, {
        key: '$height',
        get: function get() {
            return this.get('$height');
        }
    }, {
        key: '$sizeSelect',
        get: function get() {
            return this.get('$sizeSelect');
        }
    }, {
        key: '$mirror',
        get: function get() {
            return this.get('$mirror');
        }
    }]);

    return DetailProductStickerSettings;
}(DetailProductSettings);

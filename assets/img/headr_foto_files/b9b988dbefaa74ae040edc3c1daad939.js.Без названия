(function ($) {
    var googleUser = {};
    var startApp = function () {

        gapi.load('auth2', function () {
            // Retrieve the singleton for the GoogleAuth library and set up the client.
            auth2 = gapi.auth2.init({
                client_id: '127118133127-p4kjevvrv10qqupjaq6rimq7ru8qfaun.apps.googleusercontent.com',
                cookiepolicy: 'single_host_origin',
                // Request scopes in addition to 'profile' and 'email'
                //scope: 'additional_scope'
            });
            attachSignin(document.getElementById('customBtn'));
        });
    };

    function attachSignin(element) {
        console.log(element.id);
        auth2.attachClickHandler(element, {},
            function (googleUser) {
                console.log("Signed Id ", googleUser.getBasicProfile().getId());
                console.log("Signed Name ", googleUser.getBasicProfile().getName());
                console.log("Signed Email ", googleUser.getBasicProfile().getEmail());

                $.ajax({
                    url: "/social-sign-in",
                    type: "POST",
                    data: {
                        id: googleUser.getBasicProfile().getId(),
                        name: googleUser.getBasicProfile().getName(),
                        email: googleUser.getBasicProfile().getEmail(),
                        source: 2
                    },
                    success: function (data) {
                        var result = JSON.parse(data);

                        if (result['result'] == 'success')
                            window.location = 'https://www.allstick.ru/';
                    }
                });

            }, function (error) {
                //alert(JSON.stringify(error, undefined, 2));
            });
    }

    $(document).ready(function () {
        var app_starter_delay = setInterval(function () {
            if (gapi) {
                startApp();
                clearInterval(app_starter_delay);
            }
        }, 100);
    });

})(jQuery);
;
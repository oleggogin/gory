$(function () {
    var $widgets = $('.als2-city-changer')

    $widgets.each(function (i, el) {
        var $cityChanger = $(el)
        var $name = $cityChanger.find('.name')
        var $input = $cityChanger.find('input')
        $cityChanger.on('click', function () {
            var popup = $cityChanger[0].als2Popup
            popup.show().then(function ($content) {
                citySearch.attach($content.find('.als2-city-search'), function (cityId, info) {
                    popup.close()
                    $name.text(info.name)
                    $input.val(cityId)
                    $input.trigger('change')

                    var submitUrl = $cityChanger.data('submit')
                    if (submitUrl && submitUrl != '#') {
                        if (info.data && info.data.url) {
                            window.location = info.data.url
                        } else {
                            $.post(submitUrl, {cityId: $input.val()})
                        }
                    }
                })
            })
        })
    })
})
;;
"use strict";

var _createClass = function () { function defineProperties(target, props) { for (var i = 0; i < props.length; i++) { var descriptor = props[i]; descriptor.enumerable = descriptor.enumerable || false; descriptor.configurable = true; if ("value" in descriptor) descriptor.writable = true; Object.defineProperty(target, descriptor.key, descriptor); } } return function (Constructor, protoProps, staticProps) { if (protoProps) defineProperties(Constructor.prototype, protoProps); if (staticProps) defineProperties(Constructor, staticProps); return Constructor; }; }();

function _classCallCheck(instance, Constructor) { if (!(instance instanceof Constructor)) { throw new TypeError("Cannot call a class as a function"); } }

var Als2Popup = function () {
    function Als2Popup(config) {
        _classCallCheck(this, Als2Popup);

        config = config ? config : {};

        for (var key in config) {
            this[key] = config[key];
        }
    }

    _createClass(Als2Popup, [{
        key: 'show',
        value: function show() {
            var self = this;

            return new Promise(function (resolve, reject) {
                if (self.ajax) {
                    $.magnificPopup.open({
                        alignTop: true,
                        callbacks: {
                            parseAjax: function parseAjax(mfpResponse) {
                                mfpResponse.data = '<div class="als2-popup -body">' + mfpResponse.data + '</div>';
                            },
                            ajaxContentAdded: function ajaxContentAdded() {
                                resolve($(this.content));
                            }
                        },
                        items: {
                            type: 'ajax',
                            src: self.ajax
                        }
                    });
                } else {
                    reject();
                }
            });
        }
    }, {
        key: 'close',
        value: function close() {
            $.magnificPopup.close();
        }
    }]);

    return Als2Popup;
}();

$(function () {
    var $popups = $('[data-als2-popup]');

    window.als2Popup = {
        instantiate: function instantiate($elements) {
            $elements.each(function (i, el) {
                var $popup = $(el);
                var popup = new Als2Popup($popup.data('als2-popup'));
                $popup[0].als2Popup = popup;
            });
        }
    };

    window.als2Popup.instantiate($popups);
});
;;
"use strict"

$(function () {
    // быстрый костыль пока нет require
    // цель - один управляющий код, много инстансов
    var body = $('body')[0]
    if (!body['cronfy-ht-widgets']) {
        body['cronfy-ht-widgets'] = {}
    }

    var widgetData

    var widget = {
        attach: function ($widget) {
            if ($widget[0]['als2-map-yandex']) return // already attached

            widgetData = {
                initialized: false,
                map: null,
                markers: {},
                move: function (pos) {
                    this.map.setCenter([pos.lat, pos.lng])
                    this.map.setZoom(11)
                },
                closeBalloon: function (markerSid) {
                    this.markers[markerSid].mapLabel.balloon.close()
                },
                getMarkerData: function (markerSid) {
                    return this.markers[markerSid].markerSource.data
                },
                drawMarkers: function (markers) {
                    for (var markerSid in markers) {
                        var markerData = markers[markerSid]

                        if (this.markers[markerSid]) continue

                        var mapLabel = new ymaps.Placemark([markerData.lat, markerData.lng], {
                            hintContent: markerData.title,
                            balloonContent: markerData.contentString.outerHTML
                        }, {
                            iconLayout: 'default#image',
                            iconImageHref: $widget.attr('data-marker'),
                            iconImageSize: [20, 20],
                            iconImageOffset: [-10, -20]
                        })

                        this.map.geoObjects.add(mapLabel)

                        this.markers[markerSid] = {
                            mapLabel: mapLabel,
                            markerSource: markerData
                        }
                    }
                },
            }

            $widget[0]['als2-map-yandex'] = widgetData

            var self = this
            this.whenProviderReady(function () {
                self.initialize($widget)
            })
        },
        whenProviderReady: function (cb) {
            // дождемся, когда ymaps реально будет определена, и потом уже проициализируемся.
            if (typeof ymaps !== "undefined") {
                cb()
            } else {
                var self = this
                setTimeout(function () {
                    self.whenProviderReady(cb)
                }, 500)
            }
        },
        initialize: function ($widget) {
            var mapContainer = $widget[0]

            ymaps.ready(function() {
                // Создание карты.
                widgetData.map = new ymaps.Map(mapContainer, {
                    // Координаты центра карты.
                    // Порядок по умолчанию: «широта, долгота».
                    // Чтобы не определять координаты центра карты вручную,
                    // воспользуйтесь инструментом Определение координат.
                    center: [59.901567, 30.300273],
                    // Уровень масштабирования. Допустимые значения:
                    // от 0 (весь мир) до 19.
                    zoom: 11,
                    controls: [
                        'zoomControl'
                    ],
                })

                widgetData.initialized = true
                $widget.trigger('als2-map-yandex:initialized')
            });
        }
    }

    // оставим для повторного использования: если будут динамически появляться новые виджеты этого типа,
    // можно будет сделать $('body')[0][]['cronfy-ht-widgets']['als2-map'].attach($(...))
    body['cronfy-ht-widgets']['als2-map-yandex'] = widget
})
;;
